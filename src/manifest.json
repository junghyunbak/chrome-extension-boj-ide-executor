{
  "manifest_version": 3,
  "name": "BOJ IDE Executor",
  "description": "백준 문제 페이지에서 BOJ IDE를 바로 열기 위한 확장 프로그램",
  "background": { "service_worker": "background.bundle.js" },
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "32x32-disable.png"
  },
  "icons": {
    "128": "128x128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "content.styles.css",
        "32x32.png",
        "32x32-disable.png",
        "128x128.png",
        "128x128-disable.png"
      ],
      "matches": []
    }
  ]
}
